+template<int N>
+class TempBuffer
+{
+	char* heap;
+	char stack[N];
+	int heapSize;
+public:
+	TempBuffer() : heap(0) {}
+	~TempBuffer()
+	{
+		if(heap) delete[] heap;
+	}
+
+	char* get(int size)
+	{
+		if(size <= N) return stack;
+		if(heap)
+		{
+			if(size <= heapSize) return heap;
+			delete[] heap;
+		}
+		return heap = new char[heapSize = size];
+	}
+};
